{
  "name": "yxzq-utils-node",
  "version": "1.0.0",
  "main": "dist-cjs/index.js",
  "module": "dist-esm/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "dist-cjs/",
    "dist-esm/"
  ],
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist-cjs/index.js",
      "types":"./dist/index.d.ts"
    }
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "keywords": [
    "yxzq",
    "yxzq-utils",
    "js",
    "utils",
    "es",
    "ts",
    "CommonJS",
    "nodejs"
  ],
  "author": "yxzq",
  "license": "ISC",
  "description": "yxzq-utils-node",
  "dependencies": {
    "axios": "^1.7.9",
    "form-data": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  }
}
